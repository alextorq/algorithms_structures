{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-restricted-syntax": "off",
      "no-unused-vars": "off",
      "no-plusplus": "off",
      "no-param-reassign": "off",
      "max-classes-per-file": "off"
    }
}
